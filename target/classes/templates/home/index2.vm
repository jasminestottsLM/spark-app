<head>
<title>Welcome to your new home!</title>
</head>
<body>

#if ( $noUser )
	<a href="/login">Login</a>
#end

#if ( $currentUser )
	Logged in as $currentUser.getEmail()
	<br>
	My Account
#end

<h1>Apartments</h1>
#if ( $apartments )
<div>
	#foreach ( $apartment in $apartments ) 
		<a href="/apartments/$apartment.getId()">
		$apartment.getAddress()
		</a>
		<br>
	#end
</div>
#end
</body>

